+Table
    >tableKey
        tableName(string)
        tableSeatNumber(int)
        tableSeatType(int)
        tableDescription(string)

+TableTimeSlot
    >tableKey
        slot1(bool)
        slot2(bool)
        slot3(bool)
+Queue
    >queueKey
        queueOwner(string)
        queueTimestamp(timestamp)
        queueSentNumber(int)
        queueTimeSlot(string)
        queueTimestampReserve(timestamp)
        queueNote(string)
        queueName(string)
        queueTel(string)
+QueueManage
    >tableKey
        queueIsEmpty
        queueNumbers(string)
        queueName(string)
        queueReference(reference)
+QueueLog
    >queueLogKey
        tableKey(string)
        queueKey(string)
        queueSentNumber(int)
        queueTimestampReserve(timestamp)
        queueTimestampStart(timestamp)
        queueTimestampEnd(timestamp)
        serviceDuration(int)