{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/process",
        "function": "process"
      },
      {
        "source": "/pay",
        "function": "pay"
      },
      {
        "source":"/error",
        "destination":"/error.html"
      },
      {
        "source":"/cancel",
        "destination":"/cancel.html"
      },
      {
        "source":"/success",
        "destination":"/success.html"
      }
    ]
  }
}
